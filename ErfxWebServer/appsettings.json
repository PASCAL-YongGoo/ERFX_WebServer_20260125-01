{
  "ConnectionStrings": {
    "InspectionDb": "Data Source=./data/inspections.db"
  },
  "Database": {
    "RetentionDays": 90
  },
  "Mqtt": {
    "BrokerHost": "192.168.20.2",
    "BrokerPort": 1883,
    "ClientId": "ErfxWebServer",
    "Qos": 1,
    "Topics": {
      "InspectionResult": "erfx/inspector/result"
    },
    "StatusPublish": {
      "Enabled": true,
      "Topic": "erfx/webserver/status",
      "IntervalMs": 30000
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
